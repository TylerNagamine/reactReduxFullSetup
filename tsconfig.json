{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "preserve",
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "lib": ["es6", "dom"],
    "paths": {
      "history": [ "./node_modules/@types/history/index" ],
      "redux": [ "./node_modules/@types/redux/index" ],
      "react": [ "./node_modules/@types/react/index" ]
    }
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
